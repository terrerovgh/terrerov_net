# Domain Configuration
DOMAIN_NAME=your_domain.com
TRAEFIK_DOMAIN=traefik.${DOMAIN_NAME}
WEBSITE_DOMAIN=www.${DOMAIN_NAME}
PIHOLE_DOMAIN=pihole.${DOMAIN_NAME}

# Network Configuration
LAN_NETWORK=192.168.x.x/25
DOCKER_NETWORK=172.x.x.x/16

# User and Authentication
USER=your_username
EMAIL=your_email@example.com
ADMIN_PASSWORD=your_admin_password
TRAEFIK_PASSWORD=your_traefik_password
ROOT_PASSWORD=your_root_password

# API Keys and Tokens
DEEPSEEK_API_KEY=your_deepseek_api_key
CF_API_TOKEN=your_cloudflare_api_token
GITHUB_TOKEN=your_github_token

# Docker Registry Credentials
DOCKERHUB_USERNAME=your_dockerhub_username
DOCKERHUB_TOKEN=your_dockerhub_token

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_database_name

# Pi-hole Configuration
PIHOLE_PASSWORD=your_pihole_admin_password